{
  "manifest_version": 3,
  "name": "tabmeoutside",
  "version": "1.0",
  "action": {
      "default_icon": "images/icon-128.png"
  },
  "description": "Switch between tabs with custom keybindings.",
  "permissions": ["tabs"],
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "next-tab": {
      "suggested_key": {
        "default": "Alt+L"
      },
      "description": "Move to the next tab."
    },
    "previous-tab": {
      "suggested_key": {
        "default": "Alt+H"
      },
      "description": "Move to the previous tab."
    }
  }
}
